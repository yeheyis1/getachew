{
  "name": "SecureFileShare",
  "author": "Solution Ladder",
  "email": "hi@solutionladder.com",
  "version": "3.0.0",
  "homepage": "",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "cross-env GENERATE_SOURCEMAP=false next build",
    "start": "next start",
    "cra:start": "react-scripts start",
    "cra:build": "cross-env GENERATE_SOURCEMAP=false react-scripts build",
    "gb:develop": "gatsby develop",
    "gb:build": "cross-env GENERATE_SOURCEMAP=false gatsby build",
    "styleguide": "cross-env FAST_REFRESH=false styleguidist server",
    "styleguide:build": "styleguidist build",
    "prettier": "prettier --write '**/*.{js,css,html}'",
    "assets": "rimraf ./public/assets/images && cp-cli ./node_modules/thefront-assets/images ./public/assets/images",
    "leaflet-assets": "rimraf ./src/assets/images/leaflet-assets && cp-cli ./node_modules/leaflet/dist/images ./src/assets/images/leaflet-assets",
    "postinstall": "npm run assets && npm run leaflet-assets",
    "clear-all": "rimraf ./.cache ./.next ./build ./dist ./styleguide ./node_modules ./public/page-data ./public/static ./public/render-page.js ./public/render-page.js.map"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.2.1",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/lab": "^4.0.0-alpha.39",
    "@material-ui/pickers": "^3.2.10",
    "@material-ui/styles": "^4.2.1",
    "aos": "^2.3.4",
    "clsx": "^1.0.4",
    "dotenv": "^8.2.0",
    "gatsby": "^2.27.0",
    "gatsby-plugin-material-ui": "^2.1.10",
    "gatsby-plugin-page-creator": "^2.5.0",
    "gatsby-plugin-react-helmet": "^3.5.0",
    "gatsby-plugin-react-leaflet": "^2.0.13",
    "gatsby-plugin-resolve-src": "^2.1.0",
    "history": "^4.9.0",
    "jarallax": "^1.12.1",
    "leaflet": "^1.6.0",
    "next": "^10.0.1",
    "next-images": "^1.6.2",
    "node-sass": "^4.14.1",
    "npm": "^6.14.7",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.7",
    "react": "^16.13.1",
    "react-countup": "^4.3.3",
    "react-dom": "^16.8.6",
    "react-helmet": "^6.1.0",
    "react-lazy-load-image-component": "^1.5.0",
    "react-leaflet": "^2.7.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^4.0.0",
    "react-typed": "^1.2.0",
    "react-visibility-sensor": "^5.1.1",
    "start": "^5.1.0",
    "swiper": "^5.4.1",
    "thefront-assets": "git+https://github.com/gevorgmakaryan/thefront-assets.git#1.0.7",
    "uuid": "^8.3.2",
    "validate.js": "^0.13.1"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^1.11.1",
    "cp-cli": "^2.0.0",
    "cross-env": "^7.0.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "fs-extra": "^9.0.1",
    "prettier": "^1.19.1",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^5.0.0",
    "react-styleguidist": "^11.1.1",
    "rimraf": "^3.0.2",
    "typescript": "^3.5.1"
  }
}
